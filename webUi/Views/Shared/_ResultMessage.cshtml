@model AlertMessage
<div class="row mt-4 mb-2" style="justify-content: center;">
    <div class="container col-md-10">
        <div class="alert  alert-@Model.AlertType">
            <h4 class="alert-title text-center" >@Model.Title</h4>
            <p class="text-center">@Model.Message</p>
        </div>
    </div>
</div>